//
// Prefix header for all source files of the 'sequel-pro' target in the 'sequel-pro' project
//

#ifdef __OBJC__
	#import <Cocoa/Cocoa.h>
#endif

#ifdef DEBUG
	#define DLog(...) NSLog(__VA_ARGS__)
#else
	#define DLog(...) /* */
#endif

#define ALog(...) NSLog(__VA_ARGS__)

#ifndef enumerate
#define enumerate(container,var) for(NSEnumerator* _enumerator = [container objectEnumerator]; var = [_enumerator nextObject]; )
#endif

#ifndef sizeofA
#define sizeofA(a) (sizeof(a)/sizeof(a[0]))
#endif
